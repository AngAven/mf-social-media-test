{
    "rewrites": [
        {
          "source": "/api/:path*",
          "destination": "https://ms-social-media.vercel.app/:path*"
        },
        {
          "source": "/api/:path*/",
          "destination": "https://ms-social-media.vercel.app/:path*/"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
      ]
}
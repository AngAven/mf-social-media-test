{
    "routes": [{ "src": "/[^.]+", "dest": "/", "status": 200 }],
    "rewrites": [
        {
          "source": "/api/:path*",
          "destination": "https://ms-social-media.vercel.app/:path*"
        },
        {
          "source": "/api/:path*/",
          "destination": "https://ms-social-media.vercel.app/:path*/"
        }
      ]
}